<h3><%= @total %> Total Stores</h3>
<ul>
  <% @stores.each do |store| %>
    <li>
    <%= link_to store.long_name, store_path(store.id) %>
    </li>
    <ul>
      <li>
      City: <%= store.city %>
      </li>
      <li>
      Distance: <%= store.distance %>
      </li>
      <li>
      Phone number: <%= store.phone_number %>
      </li>
      <li>
      Store type: <%= store.store_type %>
      </li>
    </ul>
    <br>
  <% end %>
</ul>
